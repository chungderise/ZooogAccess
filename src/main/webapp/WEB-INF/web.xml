<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>zooogtest</servlet-name>
  	<display-name>zooogtest</display-name>
  	<description></description>
  	<servlet-class>co.jp.example.zooogtest</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ActorController</servlet-name>
  	<display-name>ActorController</display-name>
  	<description></description>
  	<servlet-class>zooog.jp.controller.ActorController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginController</servlet-name>
  	<display-name>LoginController</display-name>
  	<description></description>
  	<servlet-class>zooog.jp.controller.LoginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProjectController</servlet-name>
  	<display-name>ProjectController</display-name>
  	<description></description>
  	<servlet-class>zooog.jp.controller.ProjectController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>zooogtest</servlet-name>
  	<url-pattern>/zooogtest</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ActorController</servlet-name>
  	<url-pattern>/actor</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginController</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProjectController</servlet-name>
  	<url-pattern>/project</url-pattern>
  </servlet-mapping>
</web-app>
